<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/">Home</a></li>
      <li class="breadcrumb-item active" aria-current="page">Explore <%- gamesName%></li>
    </ol>
</nav>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3 mb-4">
    <% if(pokemons != '') { %>
        <% pokemons.forEach(function(pokemon, index) { %>
            <a href="/Pokemon/<%- pokemon.basic.name %>" class="col text-center region__link">
                <div class="region__img shadow">
                    <img src="/PokemonImages/<%- pokemon.basic.image %>" width="220" height="200"  alt=<%- pokemon.basic.name %> loading="lazy">
                </div>
                <div class="pt-1">#<%- pokemon.basic.pokedexNumber %> <%- pokemon.basic.name %></div>
            </a>
        <% }) %>
    <% } %>
</div>
